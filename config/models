User
    ident Text
    password Text Maybe
    UniqueUser ident
    deriving Typeable

Ship
    libNo Int
    name String
    UniqueShipName name

Equipment
    libNo Int
    name String
    UniqueEquipmentName name

Shipbuild
    userId UserId
    shipId ShipId
    posted UTCTime default=CURRENT_TIME
    secretary ShipId
    secretaryLv Int
    headquartersLv Int
    resourceId ResourceId

Develop
    userId UserId
    equipmentId EquipmentId
    posted UTCTime default=CURRENT_TIME
    secretary ShipId
    secretaryLv Int
    headquartersLv Int
    resourceId ResourceId

Resource
    fuel Int
    ammunition Int
    steel Int
    bauxite Int
    UniqueResource fuel ammunition steel bauxite

 -- By default this file is used in Model.hs (which is imported by Foundation.hs)
