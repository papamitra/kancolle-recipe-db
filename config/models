User
    ident Text
    password Text Maybe
    UniqueUser ident
    deriving Typeable

Ship
    libNo Int
    name String
    UniqueShipName name

Equipment
    libNo Int
    name String
    UniqueEquipmentName name

Shipbuild
    shipId ShipId
    created UTCTime default=CURRENT_TIME
    secretary ShipId
    secretaryLv Int
    headquartersLv Int
    resourceId ResourceId

Develop
    equipmentId 
    created UTCTime default=CURRENT_TIME
    secretary ShipId
    secretaryLv Int
    headquartersLv Int
    resourceId ResourceId

Resource
    fuel Int
    ammunition Int
    steel Int
    bauxite Int

